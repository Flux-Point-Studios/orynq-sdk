{
  "name": "@poi-sdk/payer-cardano-cip30",
  "version": "0.0.0",
  "description": "CIP-30 browser wallet adapter for Cardano payments in poi-sdk",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rimraf dist"
  },
  "keywords": [
    "poi",
    "cardano",
    "cip30",
    "wallet",
    "nami",
    "eternl",
    "lace",
    "vespr",
    "payments"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@poi-sdk/core": "workspace:*"
  },
  "peerDependencies": {
    "lucid-cardano": ">=0.10.0"
  },
  "devDependencies": {
    "lucid-cardano": "^0.10.7",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
